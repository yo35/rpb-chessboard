msgstr "Символы фигур"

#: templates/adminpage/options/general/piecesymbols.php
msgid "English initials"
msgstr "Английские сокращения"

#: templates/adminpage/options/general/piecesymbols.php
msgid "Localized initials"
msgstr "Локализованные сокращения"

#: templates/adminpage/options/general/piecesymbols.php
msgid "Figurines"
msgstr "Фигуры"

#: templates/adminpage/options/general/piecesymbols.php
